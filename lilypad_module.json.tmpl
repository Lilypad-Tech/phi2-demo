{
    "machine": {
        "gpu": 1,
        "cpu": 1000,
        "ram": 100
    },
    "job": {
        "APIVersion": "V1beta1",
        "Metadata": {
            "CreatedAt": "2023-01-01T00:00:00Z",
            "Requester": {}
        },
        "Spec": {
            "Deal": {
                "Concurrency": 1
            },
            "Docker": {
                "EnvironmentVariables": [
                    "{{if .Prompt}}{{.Prompt}}{{else}}PROMPT='Default prompt for phi2-demo'{{end}}",
                    "RANDOM_SEED=42"
                ],
                "Entrypoint": [
                    "python3",
                    "/usr/src/app/loadmodel.py",
                    "\"{{.Prompt}}\""
                ],
                "Image": "noryev/phi2-demo:latest"
            },
            "Engine": "Docker",
            "Network": {
                "Type": "None"
            },
            "PublisherSpec": {
                "Type": "Estuary"
            },
            "Resources": {
                "GPU": "1"
            },
            "Timeout": 1800,
            "Verifier": "Noop",
            "inputs": [],
            "outputs": [
                {
                    "Name": "output",
                    "StorageSource": "IPFS",
                    "path": "/output"
                }
            ]
        }
    }
}
