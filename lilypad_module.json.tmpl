{
    "machine": {
        "gpu": 1,
        "cpu": 1000,
        "ram": 100
    },
    "job": {
        "APIVersion": "V1beta1",
        "Metadata": {
            "CreatedAt": "2023-01-01T00:00:00Z",
            "Requester": {}
        },
        "Spec": {
            "Deal": {
                "Concurrency": 1
            },
            "Docker": {
                "Entrypoint": ["bash", "-c", "python3 /usr/src/app/loadmodel.py 2>/dev/null"],
                "Image": "noryev/phi2-demo:latest"
                 "EnvironmentVariables": [
          "MODEL_PATH=/usr/src/app/model",
          "ANOTHER_ENV_VAR=value",
          "OUTPUT_DIR=/outputs/",
          "HF_HUB_OFFLINE=1"
        ]
            },
            "Engine": "Docker",
            "Network": {
                "Type": "None"
            },
            "PublisherSpec": {
                "Type": "Estuary"
            },
            "Resources": {
                "GPU": "1"
            },
            "Timeout": 1800,
            "Verifier": "Noop",
            "inputs": [],
            "outputs": [
                {
                    "Name": "output",
                    "StorageSource": "IPFS",
                    "path": "/output"
                }
            ]
        }
    }
}
